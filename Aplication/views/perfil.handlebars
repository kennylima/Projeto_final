<h2>Seja bem vindo +nome</h2>
<a class="btn btn-primary btn-sm" href="/" type="submit">Sair</a>
<hr>
<h4>Seus dados</h4>
<p>Nome:{{associados.nome}}</p>
<p>E-mail:{{associados.email}}</p>
<p>Telefone:{{associados.telefone}}</p>
<p>Matrícula da empresa:{{associados.matriculaEmpresa}}</p>
<p>CPF:{{associados.cpf}}</p>
<a class="w-100 btn btn-lg btn-primary" href="/perfil/edit" type="submit">Editar dados</a>
<br><br>
<hr>
<h4>Dependentes</h4>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Nome</th>
                <th scope="col">Telefone</th>
                <th scope="col">Parentesco</th>
                <th scope="col">Ações</th>
            </tr>
        </thead>
        <tbody>
            {{#each dependentes}}
            <tr>
                <td>{{this.nome}}</td>
                <td>{{this.telefone}}</td>
                <td>{{this.parentesco}}</td>
                <td><a href="/perfil/dependente/edit/{{this.id}}">Editar</a> | <a href="/perfil/dependente/delete/{{this.id}}">Excluir</a></td>
            </tr>
            {{/each}}
        </tbody> 
    </table>
</div>
<a class="w-100 btn btn-lg btn-primary" href="/perfil/dependente" type="submit">Cadastrar dependente</a>
<hr>
<h4>Reservas</h4>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Churrasqueira</th>
                <th scope="col">Data</th>
                <th scope="col">Ação</th>
            </tr>
        </thead>
        <tbody>
            {{#each reservas}}
            <tr>
                <td>{{this.local}}</td>
                <td>{{this.data}}</td>
                <td><a href="/perfil/reservas/delete/{{this.id}}">Excluir</a></td>
            </tr>
            {{/each}}
        </tbody> 
    </table>
</div>
<a class="w-100 btn btn-lg btn-primary" href="/perfil/reservas" type="submit">Reservar churrasqueira</a>
<hr>