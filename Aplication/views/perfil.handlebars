<h4>Dependentes</h4>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Nome</th>
                <th scope="col">Telefone</th>
                <th scope="col">Parentesco</th>
            </tr>
        </thead>
        <tbody>
            {{#each dependente}}
            <tr>
                <td>{{this.nome}}</td>
                <td>{{this.telefone}}</td>
                <td>{{this.parentesco}}</td>
                <td><a href="/dependente/edit/{{this.id}}">Editar</a> | <a href="/dependente/delete/{{this.id}}">Excluir</a></td>
            </tr>
            {{/each}}
        </tbody> 
    </table>
</div>
<a class="w-100 btn btn-lg btn-primary" href="/perfil/dependente" type="submit">Cadastrar dependente</a>
<hr>
<h4>Reservas</h4>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Churrasqueira</th>
                <th scope="col">Data</th>
            </tr>
        </thead>
        <tbody>
            {{#each usuarios}}
            <tr>
                <td>{{this.id_usuario}}</td>
                <td>{{this.nome_usuario}}</td>
                <td><a href="/usuario/edit/{{this.id_usuario}}">Editar</a> | <a href="/usuario/delete/{{this.id_usuario}}">Excluir</a></td>
            </tr>
            {{/each}}
        </tbody> 
    </table>
</div>
<a class="w-100 btn btn-lg btn-primary" href="/perfil/reservas" type="submit">Reservar churrasqueira</a>
<hr>