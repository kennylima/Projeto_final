<h2>Resultados</h2>
<hr>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th scope="col">Associado</th>
                <th scope="col">Status</th>
                <th scope="col">Dependentes</th>
            </tr>
        </thead>
        <tbody>
            {{#each associado}}
            <tr>
                <td>{{this.nome}}</td>
                <td>{{this.status}}</td>
                <td>{{this.nome-dependente}}</td>
            </tr>
            {{/each}}
        </tbody> 
    </table>
</div>

<a class="w-100 btn btn-lg btn-primary" href="/perfil/administrador/2" type="submit">Retornar</a>
